{
  "manifest_version": 3,
  "name": "Emoji Tab Marker",
  "version": "1.0",
  "description": "Allows users to mark pages with an emoji in the tab title.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["emojis.js", "content_script.js"],
      "css": ["content_script.css"]
    }
  ]
}

